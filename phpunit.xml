<?xml version="1.0"?>
<phpunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  processIsolation="true" stderr="true" xsi:noNamespaceSchemaLocation=
  "https://schema.phpunit.de/9.3/phpunit.xsd" colors="true">
  <coverage/>
  <php>
    <const name="PREFIXPATH" value="tests/galleries-test/"/>
    <env name="APP_ENV" value="test"/>
    <env name="APP_ENV" value="dev"/>
    <ini name="output_buffering" value="On"/>
  </php>
  <testsuites>
    <testsuite name="unit">
      <directory>tests/Unit</directory>
    </testsuite>
    <testsuite name="integration">
      <directory>tests/Integration</directory>
    </testsuite>
  </testsuites>
  <logging>
    <testdoxText outputFile="tests/result-of-tests-performed.txt"/>
  </logging>
</phpunit>
